{
  "name": "jimi-web-changelog",
  "version": "0.0.7",
  "author": "Feax",
  "description": "changelog",
  "main": "dist/index.js",
  "module": "es/index.js",
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "cz": "git add . && git cz",
    "log": "conventional-changelog --config lib/log -i CHANGELOG.md -s -r 0",
    "build": "cross-env babel lib --out-dir dist",
    "v": "standard-version -r patch"
  },
  "keywords": [
    "changelog",
    "git"
  ],
  "config": {
    "commitizen": {
      "path": "./dist/cz"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "commitizen": "^3.0.5",
    "commitlint": "^7.3.1",
    "compare-func": "^1.3.2",
    "conventional-changelog-cli": "^2.0.11",
    "cross-env": "^5.2.0",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3",
    "github-url-from-git": "^1.5.0",
    "husky": "^1.3.1",
    "jimi-web-changelog": "^0.0.3",
    "lint-staged": "^8.1.0",
    "longest": "^2.0.1",
    "q": "^1.5.1",
    "right-pad": "^1.0.1",
    "standard-version": "^4.4.0",
    "work-wrap": "^1.0.0"
  },
  "license": "ISC"
}
