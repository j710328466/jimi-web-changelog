{
  "name": "jimi-web-changelog",
  "version": "0.0.3",
  "author": "Feax",
  "description": "changelog",
  "main": "dist/index.js",
  "module": "es/index.js",
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "cz": "git add . && git cz",
    "lint": "lint-staged",
    "log": "conventional-changelog -p jimi -i CHANGELOG.md -s -r 0",
    "build": "cross-env babel lib --out-dir dist"
  },
  "keywords": [
    "changelog",
    "git"
  ],
  "config": {
    "commitizen": {
      "path": "./dist/cz"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "commitizen": "^3.0.5",
    "cross-env": "^5.2.0",
    "husky": "^1.3.1",
    "jimi-web-changelog": "^0.0.3",
    "lint-staged": "^8.1.0",
    "longest": "^2.0.1",
    "right-pad": "^1.0.1",
    "work-wrap": "^1.0.0"
  },
  "license": "ISC"
}
